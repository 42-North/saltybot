{
  "manifest_version": 2,

  "name": "Salty Bot",
  "description": "salt",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "run_at": "document_end",
        "matches": ["http://www.saltybet.com/*"],
        "js": ["jquery-2.1.3.min.js", "content.js"]
    }
]
}